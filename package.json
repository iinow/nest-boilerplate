{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "local": "cross-env NODE_ENV=local nest start --watch",
    "dev": "cross-env NODE_ENV=dev nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=local jest",
    "test:watch": "cross-env NODE_ENV=local jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage --forceExit --detectOpenHandles",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.13.0",
    "@liaoliaots/nestjs-redis": "^3.0.0-next.3",
    "@nestjs/axios": "0.0.1",
    "@nestjs/common": "^8.0.3",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.3",
    "@nestjs/elasticsearch": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.3",
    "@nestjs/platform-socket.io": "^8.0.4",
    "@nestjs/platform-ws": "^8.0.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.3",
    "@nestjs/terminus": "^8.0.0-next.0",
    "@nestjs/typeorm": "^8.0.1",
    "@nestjs/websockets": "^8.0.4",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "discord-nestjs": "^1.1.12",
    "discord.js": "^12.5.3",
    "express-handlebars": "^5.3.2",
    "hbs": "^4.1.2",
    "influx": "^5.9.2",
    "influx-syslog": "^0.2.4",
    "ioredis": "^4.27.6",
    "js-yaml": "^4.1.0",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-kakao": "^1.0.1",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.1.3",
    "socket.io-redis": "^6.1.1",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2",
    "ws": "^7.5.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/express": "^4.17.8",
    "@types/express-handlebars": "^5.3.1",
    "@types/hbs": "^4.0.1",
    "@types/ioredis": "^4.26.6",
    "@types/jest": "^26.0.15",
    "@types/js-yaml": "^4.0.2",
    "@types/node": "^14.14.6",
    "@types/passport-kakao": "^0.2.1",
    "@types/redis": "^2.8.31",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "^~(.*)$": "<rootDir>$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
